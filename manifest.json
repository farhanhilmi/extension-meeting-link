{
  "name": "Meeting Links Extension",
  "author": "Farhan Hilmi",
  "version": "1.1",
  "description": "Extension to save your link meeting links so they can be easily managed and accessed in your browser!",
  "permissions": ["activeTab", "storage"],

  "content_security_policy": "script-src 'self'; script-src-elem 'self' 'unsafe-inline' http://cdn.jsdelivr.net/timepicker.js/latest/timepicker.min.js; object-src 'self'",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Meeting Links",
    "default_icon": {
      "16": "img/16.png",
      "48": "img/48.png",
      "128": "img/128.png"
    }
  },
  "manifest_version": 2
}
